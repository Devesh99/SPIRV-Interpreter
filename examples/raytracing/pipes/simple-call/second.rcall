#version 460 core
#extension GL_EXT_ray_tracing : enable

layout(location = 0) callableDataInEXT vec4 outColor;

void main() {
    outColor = vec4(
        float(gl_LaunchIDEXT.x) / gl_LaunchSizeEXT.x,
        float(gl_LaunchIDEXT.y) / gl_LaunchSizeEXT.y,
        0.0,
        0.0
    );
}
